<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">



<mapper namespace="com.chanelcode.mybatis.dao.EmployeeMapper">
    <select id="getEmpById" resultType="com.chanelcode.mybatis.bean.Employee">
        select id,last_name lastName,email,gender from mybatis.employee where id = #{id}
    </select>
</mapper>
<!--

namespcace:名称空间，指定为接口全类名
id 唯一标识
resultType 返回值类型
#{id} 传过来的id值

-->